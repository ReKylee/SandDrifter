; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="no bullshit drifting"
config/description="Small car game"
run/main_scene="res://node_3d.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0.262745, 0.317647, 0.356863, 1)

[aseprite]

import/import_plugin/enable_automatic_importer=true

[audio]

buses/default_bus_layout=""

[autoload]

DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/BurstParticles2D/plugin.cfg", "res://addons/QuickPluginManager/plugin.cfg", "res://addons/compass/plugin.cfg", "res://addons/dynamic_day_night_cycles/plugin.cfg", "res://addons/extra_export_hints/plugin.cfg", "res://addons/radial_menu/plugin.cfg", "res://addons/softbody2d/plugin.cfg", "res://addons/trail_2d/plugin.cfg", "res://addons/vizpath/plugin.cfg", "res://addons/PaletteTools/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/sprite_painter/plugin.cfg", "res://addons/AsepriteWizard/plugin.cfg", "res://addons/Rotor Node/plugin.cfg", "res://addons/easy_velocity_vector_display/plugin.cfg", "res://addons/freecam_3D/plugin.cfg", "res://addons/draw3d/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/goutte.camera.trackball/plugin.cfg")

